{
	"name": "portfolio",
	"dockerComposeFile": [
		"../docker-compose.yaml"
	],
	"service": "dev-container",
	"workspaceFolder": "/workspaces/portfolio",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.profiles.linux": {
					"bash": {
						"path": "bash",
						"icon": "terminal-bash"
					}
				}
			},
			"extensions": [
				"yzhang.markdown-all-in-one",
				"ms-dotnettools.csharp",
				"ms-dotnettools.csdevkit"
			]
		}
	}
}
