{
	"name": "armckinneygithubio",
	"dockerComposeFile": [
		"docker-compose.yaml"
	],
	"service": "devcontainer",
	"workspaceFolder": "/workspaces/armckinneygithubio",
	"customizations": {
		"vscode": {
			"settings": {
				"terminal.integrated.profiles.linux": {
					"bash": {
						"path": "bash",
						"icon": "terminal-bash"
					}
				},
				"files.exclude": {
					"**/.git": true,
					"**/.svn": true,
					"**/.hg": true,
					"**/CVS": true,
					"**/.DS_Store": true,
					"**/bin": true,
					"**/obj": true,
					"**/.vs": true
				}
			},
			"extensions": [
				"ms-azuretools.vscode-docker",
				"yzhang.markdown-all-in-one",
				"github.vscode-github-actions",
				"ms-dotnettools.csharp",
				"ms-dotnettools.csdevkit"
			]
		}
	}
}